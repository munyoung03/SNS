<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_mainpage">

    <fragment
        android:id="@+id/navigation_mainpage"
        android:name="com.example.sns.view.fragment.MainPageFragment"
        android:label="@string/title_mainpage"
        tools:layout="@layout/fragment_mainpage" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.sns.view.fragment.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.example.sns.view.fragment.MyPageFragment"
        android:label="@string/title_mypage"
        tools:layout="@layout/fragment_mypage" />

    <fragment
        android:id="@+id/navigation_chatting"
        android:name="com.example.sns.view.fragment.ChattingFragment"
        android:label="@string/title_chatting"
        tools:layout="@layout/fragment_chatting"/>
</navigation>